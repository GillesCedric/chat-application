{
	"breadcrumbs.enabled": true,
	"editor.bracketPairColorization.enabled": true,
	"editor.formatOnPaste": true,
	"editor.formatOnType": true,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": "active",
	"editor.insertSpaces": false,
	"editor.linkedEditing": true,
	"editor.minimap.enabled": false,
	"editor.quickSuggestions": {
		"other": true,
		"comments": true,
		"strings": true
	},
	"editor.stickyScroll.enabled": true,
	"editor.suggestSelection": "first",
	"editor.suggest.preview": true,
	"editor.tabSize": 2,
	"explorer.autoReveal": false,
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.expand": false,
	"explorer.fileNesting.patterns": {
		"*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts, ${capture}.d.ts.map",
		"*.jsx": "${capture}.js",
		"*.ts": "${capture}.ts.backup, ${capture}.less, ${capture}.css, ${capture}.css.map, ${capture}.module.less, ${capture}.module.css, ${capture}.module.css.map, ${capture}.wiki.md, ${capture}.verification.http",
		"*.tsx": "${capture}.tsx.backup, ${capture}.ts, ${capture}.less, ${capture}.css, ${capture}.css.map, ${capture}.module.less, ${capture}.module.css, ${capture}.module.css.map",
		"tsconfig.json": "tsconfig.*.json",
		"composer.json": "composer.lock",
		"package.json": "package-lock.json, yarn.lock",
		"*.less": "${capture}.css, ${capture}.css.map",
		"*.css": "${capture}.css.map",
		"*.json": "${capture}.json.backup",
		"*.dict": "${capture}.dict.backup",
		".gitignore": ".gitignore.backup",
	},
	"files.associations": {
		"*.inc": "php",
		"*.less.backup": "less",
		"*.less.ignore": "less",
		"*.css.backup": "css",
		"*.css.ignore": "css",
	},
	"files.eol": "\n",
	"git.mergeEditor": true,
	"html.format.indentHandlebars": true,
	"html.format.indentInnerHtml": true,
	"javascript.inlayHints.enumMemberValues.enabled": true,
	"javascript.inlayHints.functionLikeReturnTypes.enabled": true,
	"javascript.inlayHints.parameterNames.enabled": "all",
	"javascript.inlayHints.parameterTypes.enabled": true,
	"javascript.inlayHints.propertyDeclarationTypes.enabled": true,
	"javascript.inlayHints.variableTypes.enabled": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"less.compile": {
		"out": false,
		"compress": true,
		"sourceMap": true,
		"relativeUrls": false,
		"autoprefixer": "> 5%, last 2 versions, Firefox ESR, not ie <= 11, not dead",
		"ieCompat": false
	},
	"less.lint.duplicateProperties": "warning",
	"less.lint.idSelector": "error",
	"less.lint.ieHack": "error",
	"less.lint.important": "warning",
	"less.lint.universalSelector": "error",
	"less.lint.zeroUnits": "warning",
	"markdown.extension.print.theme": "dark",
	"npm.enableRunFromFolder": true,
	"search.exclude": {
		"**/archive": true,
		"**/build": true,
		"**/dist": true,
		"**/*.backup": true,
	},
	"search.useIgnoreFiles": false,
	"spellright.documentTypes": [
		"markdown",
		"latex",
		"plaintext",
		"json",
		"bat",
		"php",
		"javascript",
		"typescript",
		"less",
		"typescriptreact",
		"powershell"
	],
	"spellright.language": [
		"en",
		"fr",
	],
	"task.autoDetect": "on",
	"terminal.integrated.smoothScrolling": true,
	"todo-tree.general.tags": [
		"BUG",
		"HACK",
		"FIXME",
		"TODO",
		"ZYXW",
	],
	"todo-tree.regex.regex": "(//|#|<!--|-- |\\*\\s+|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
	"typescript.inlayHints.enumMemberValues.enabled": true,
	"typescript.inlayHints.functionLikeReturnTypes.enabled": true,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.inlayHints.parameterTypes.enabled": true,
	"typescript.inlayHints.propertyDeclarationTypes.enabled": true,
	"typescript.inlayHints.variableTypes.enabled": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"workbench.iconTheme": "vscode-icons",
	"cSpell.words": [
		"apigw",
		"keytar"
	],
}